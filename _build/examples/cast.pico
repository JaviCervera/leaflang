int = 100
real = 365.25
string = "Hello"

Print("Int to int: " + (int%)$)
Print("Int to real: " + (int#)$)
Print("Int to string: " + int$)

Print("Real to int: " + (real%)$)
Print("Real to real: " + (real#)$)
Print("Real to string: " + real$)

Print("String to int: " + (string%)$)
Print("String to real: " + (string#)$)
Print("String to string: " + string$)
