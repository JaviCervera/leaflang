function CreateListAndAddElement()
    list = ["One", "Two", "Three"]
    list[ListSize(list)] = "Last"
    Print(ListSize(list)|s)
    for i = 0 to ListSize(list) - 1 do
        Print(list[i]|s)
    end
end

