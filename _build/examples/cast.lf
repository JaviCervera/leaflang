int = 100
float = 365.25
string = "Hello"

Print("Int to int: " + (int|i)|s)
Print("Int to float: " + (int|f)|s)
Print("Int to string: " + int|s)

Print("Float to int: " + (float|i)|s)
Print("Float to float: " + (float|f)|s)
Print("Float to string: " + float|s)

Print("String to int: " + (string|i)|s)
Print("String to float: " + (string|f)|s)
Print("String to string: " + string|s)
